<p id="notice"><%= notice %></p>

<h1>Cambio Director Tesis Registros</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th>Nombre segundo director</th>
      <th>Caso</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @cambio_director_tesis_registros.each do |cambio_director_tesis_registro| %>
      <tr>
        <td><%= cambio_director_tesis_registro.nombre %></td>
        <td><%= cambio_director_tesis_registro.nombre_segundo_director %></td>
        <td><%= cambio_director_tesis_registro.caso_id %></td>
        <td><%= link_to 'Mostrar', cambio_director_tesis_registro %></td>
        <td><%= link_to 'Editar', edit_cambio_director_tesis_registro_path(cambio_director_tesis_registro) %></td>
        <td><%= link_to 'Eliminar', cambio_director_tesis_registro, method: :delete, data: { confirm: '¿Está seguro?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Nuevo Cambio Director Tesis Registro', new_cambio_director_tesis_registro_path %>
