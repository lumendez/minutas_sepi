

<h1>Tipo Usuarios</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tipo_usuarios.each do |tipo_usuario| %>
      <tr>
        <td><%= tipo_usuario.nombre %></td>
        <td><%= link_to 'Mostrar', tipo_usuario %></td>
        <td><%= link_to 'Editar', edit_tipo_usuario_path(tipo_usuario) %></td>
        <td><%= link_to 'Eliminar', tipo_usuario, method: :delete, data: { confirm: '¿Está seguro?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Nuevo Tipo Usuario', new_tipo_usuario_path %>
