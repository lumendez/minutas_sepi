

<h1>Segundo Director Tesis Registros</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th>Caso</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @segundo_director_tesis_registros.each do |segundo_director_tesis_registro| %>
      <tr>
        <td><%= segundo_director_tesis_registro.nombre %></td>
        <td><%= segundo_director_tesis_registro.caso_id %></td>
        <td><%= link_to 'Mostrar', segundo_director_tesis_registro %></td>
        <td><%= link_to 'Editar', edit_segundo_director_tesis_registro_path(segundo_director_tesis_registro) %></td>
        <td><%= link_to 'Eliminar', segundo_director_tesis_registro, method: :delete, data: { confirm: '¿Está seguro?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Nuevo Segundo Director Tesis Registro', new_segundo_director_tesis_registro_path %>
