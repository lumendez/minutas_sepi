

<h1>Cambio Tema Tesis Registros</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th>Caso</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @cambio_tema_tesis_registros.each do |cambio_tema_tesis_registro| %>
      <tr>
        <td><%= cambio_tema_tesis_registro.nombre %></td>
        <td><%= cambio_tema_tesis_registro.caso_id %></td>
        <td><%= link_to 'Mostrar', cambio_tema_tesis_registro %></td>
        <td><%= link_to 'Editar', edit_cambio_tema_tesis_registro_path(cambio_tema_tesis_registro) %></td>
        <td><%= link_to 'Eliminar', cambio_tema_tesis_registro, method: :delete, data: { confirm: '¿Está seguro?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Nuevo Cambio Tema Tesis Registro', new_cambio_tema_tesis_registro_path %>
