<p id="notice"><%= notice %></p>

<h1>Cursar Unidad Aprendizaje Externa Registros</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th>Clave</th>
      <th>Creditos</th>
      <th>Unidad academica</th>
      <th>Caso</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @cursar_unidad_aprendizaje_externa_registros.each do |cursar_unidad_aprendizaje_externa_registro| %>
      <tr>
        <td><%= cursar_unidad_aprendizaje_externa_registro.nombre %></td>
        <td><%= cursar_unidad_aprendizaje_externa_registro.clave %></td>
        <td><%= cursar_unidad_aprendizaje_externa_registro.creditos %></td>
        <td><%= cursar_unidad_aprendizaje_externa_registro.unidad_academica %></td>
        <td><%= cursar_unidad_aprendizaje_externa_registro.caso_id %></td>
        <td><%= link_to 'Mostrar', cursar_unidad_aprendizaje_externa_registro %></td>
        <td><%= link_to 'Editar', edit_cursar_unidad_aprendizaje_externa_registro_path(cursar_unidad_aprendizaje_externa_registro) %></td>
        <td><%= link_to 'Eliminar', cursar_unidad_aprendizaje_externa_registro, method: :delete, data: { confirm: '¿Está seguro?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

